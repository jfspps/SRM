    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script>
        var dataSet = [["James","Bob","jamesbob@email.com"],
            ["Jane","Bob","jane12432@email.com"],
            ["Tim","Tom","timetomtam@tim.co.uk"],
            ["Jake","Josh","whizbang@pop.com"],
            ["Chris","Smith","chris@someplace.com"],
            ["Jill","Jungle","Jill93@email.net"],
            ["Sam","Dodds","samdodds21@threo.net"]
        ];

        $(document).ready(function() {
            $('#example').DataTable( {
